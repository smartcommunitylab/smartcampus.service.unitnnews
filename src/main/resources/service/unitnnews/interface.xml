<?xml version="1.0" encoding="UTF-8"?>
<service-definition xmlns="http://www.example.org/interface-schema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.example.org/interface-schema ../interface-schema.xsd ">

	<interface serviceURI="smartcampus.service.unitnnews">
		<operation name="GetOperaNews" storable="true">
			<output type="smartcampus.service.unitnnews.data.message.Unitnnews$NewsEntry" repeated="true"/>
		</operation>
		<operation name="GetUnitnNews" storable="true">
			<parameter name="baseurl" type="String" />
			<parameter name="variableurl" type="String" />
			<parameter name="source" type="String" />
			<output type="smartcampus.service.unitnnews.data.message.Unitnnews$NewsEntry" repeated="true"/>
		</operation>		
		<operation name="GetCiscaNews" storable="true">
			<output type="smartcampus.service.unitnnews.data.message.Unitnnews$NewsEntry" repeated="true"/>
		</operation>		
		<operation name="GetDisiNews" storable="true">
			<output type="smartcampus.service.unitnnews.data.message.Unitnnews$NewsEntry" repeated="true"/>
		</operation>		
	</interface>

	<implementation package="smartcampus.service.unitnnews">
		<operationImpl name="GetOperaNews"/>
		<operationImpl name="GetUnitnNews"/>
		<operationImpl name="GetCiscaNews"/>
		<operationImpl name="GetDisiNews"/>
	</implementation>

</service-definition>
